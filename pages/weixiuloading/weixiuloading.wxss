/* miniprogram/pages/weixiuloading/weixiuloading.wxss */
page {
  /*background:burlywood;*/
  height: 100%;
}
.container{
  overflow: hidden;
  background-image: url(data:image/jpg;base64,);
  
  height: 100vh;
  width: 100%;
  background-repeat: no-repeat;
  background-size: 100% 100%;

}

.header{
  width: 180rpx;
  height: 180rpx;
  border: 2px solid black;
  border-radius: 60%;
  background-color: white;
  margin: 80rpx auto;
  margin-bottom: 80rpx;
  display: block;
}

.loginPane{
  width: 500rpx;
  height:500rpx;
  margin:0 auto ;
  display: flex;
  flex-direction: column;
}

.loginPane .formItem{
  flex: 1;
  display: flex;
  /*background-color:white;*/
  /*border-bottom:1px solid cyan ;*/
  margin:20rpx 0;
}

.password,
.account{
  border-bottom: 1px solid tomato;
  color: tomato;
  flex: 1;
  margin-left: 20rpx;
}

.spaceBetween{
  justify-content: space-between;
}

.registerBtn,
.loginBtn{
  width: 500rpx;
  border-radius: 40rpx;
}
